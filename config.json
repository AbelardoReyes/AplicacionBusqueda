{
  "mercadolibre": {
    "url": "https://www.mercadolibre.com.mx",
    "actions": [
      {
        "action": "send_keys",
        "selector": "input[name='as_word']",
        "value": "Nintendo Switch"
      },
      {
        "action": "submit",
        "selector": "form.nav-search"
      },
      {
        "action": "click",
        "selector": "a.ui-search-link[title*='Hasta 4000']"
      }
    ],
    "fields": {
      "title": ".ui-search-item__title",
      "price": ".ui-search-price__second-line .andes-money-amount__fraction",
      "currency": ".ui-search-price__second-line .andes-money-amount__currency-symbol",
      "list_price": ".andes-money-amount--previous .andes-money-amount__fraction",
      "list_currency": ".andes-money-amount--previous .andes-money-amount__currency-symbol",
      "rating": ".ui-search-reviews__rating-number"
    }
  }
}
